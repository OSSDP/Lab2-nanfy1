import java.util.Arrays;

public class SolutionTest {
    public static void main(String[] args) {
        Solution solution = new Solution();

        // 测试用例 1
        int[] nums1 = {2, 0, 2, 1, 1, 0};
        solution.sortColors(nums1);
        System.out.println("Test Case 1: " + Arrays.toString(nums1)); // 输出应为 [0, 0, 1, 1, 2, 2]

        // 测试用例 2
        int[] nums2 = {2, 0, 1};
        solution.sortColors(nums2);
        System.out.println("Test Case 2: " + Arrays.toString(nums2)); // 输出应为 [0, 1, 2]

        // 测试用例 3
        int[] nums3 = {1, 2, 0, 1, 0, 2};
        solution.sortColors(nums3);
        System.out.println("Test Case 3: " + Arrays.toString(nums3)); // 输出应为 [0, 0, 1, 1, 2, 2]

        // 测试用例 4
        int[] nums4 = {0, 0, 0, 0};
        solution.sortColors(nums4);
        System.out.println("Test Case 4: " + Arrays.toString(nums4)); // 输出应为 [0, 0, 0, 0]

        // 测试用例 5
        int[] nums5 = {1, 1, 1, 1};
        solution.sortColors(nums5);
        System.out.println("Test Case 5: " + Arrays.toString(nums5)); // 输出应为 [1, 1, 1, 1]

        // 测试用例 6
        int[] nums6 = {2, 2, 2, 2};
        solution.sortColors(nums6);
        System.out.println("Test Case 6: " + Arrays.toString(nums6)); // 输出应为 [2, 2, 2, 2]

        // 测试用例 7
        int[] nums7 = {};
        solution.sortColors(nums7);
        System.out.println("Test Case 7: " + Arrays.toString(nums7)); // 输出应为 []
    }
}
